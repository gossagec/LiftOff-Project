@using LiftOff_Project.Models;
@model List<Song>

<h1>Song List</h1>

<table class="table">
    <tr>
        <th>Artist</th>
        <th>Title</th>
    </tr>

    @foreach (var song in Model)
    {
        <tr>
            <td>@song.Artist</td>
            <td>@song.Title</td>
        </tr>
    }
</table>