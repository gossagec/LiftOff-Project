@model IEnumerable<LiftOff_Project.Models.Song>;

@{
    ViewData["Title"] = "Index";
}

<h1>Song List</h1>

@if (Model != null && Model.Any())
{
    <table class="table">
        <tr>
        <th>Artist</th>
        <th>Title</th>
    </tr>

    
        @foreach (var song in Model)
        {
            <tr>
                <td>@song.Artist</td>
                <td>@song.Title</td>
            </tr>
        }
    
</table>
}
else
{
    <p>No Songs Found</p>
}